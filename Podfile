# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'SlackFirebase' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for SlackFirebase
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Database'

  target 'SlackFirebaseTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'SlackFirebaseUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
